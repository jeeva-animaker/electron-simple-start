{
  "name": "sample-chrome-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch-background": "parcel watch --target=background --no-hmr src/background/index.html",
    "build-background": "parcel build --target=background --no-source-maps src/background/index.html",
    "watch-app": "parcel watch --target=app --no-hmr src/app/index.js",
    "build-app": "parcel build --target=app --no-source-maps src/app/index.js",
    "watch-video": "parcel watch --target=video --no-hmr src/video/index.html",
    "build-video": "parcel build --target=video --no-source-maps src/video/index.html",
    "start": "concurrently \"npm:watch-*\"",
    "build": "concurrently \"npm:build-*\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "targets": {
    "background": {
      "distDir": "extension/dist/background",
      "publicUrl": "./"
    },
    "app": {
      "distDir": "extension/dist/content_scripts/app"
    },
    "video": {
      "distDir": "extension/dist/public/video",
      "publicUrl": "./"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@jeeva-animaker.github.com:jeeva-animaker/electron-simple-start.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@parcel/core": "*",
    "classnames": "^2.3.1",
    "concurrently": "^6.0.2",
    "lodash": "^4.17.21",
    "parcel": "^2.0.0-beta.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recordrtc": "^5.6.2",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13"
  }
}