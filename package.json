{
  "name": "electron-calculator",
  "version": "1.0.0",
  "description": "",
  "main": "electron/index.js",
  "scripts": {
    "start": "parcel watch --target=electron src/index.js",
    "build": "parcel build --target=electron --no-source-maps src/index.js",
    "electron": "electron ./electron",
    "pack": "electron-builder --dir=./electron",
    "dist": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "targets": {
    "electron": {
      "distDir": "electron/dist"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "electron": "^12.0.4",
    "electron-builder": "^22.10.5"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "parcel": "^2.0.0-beta.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.2.3"
  },
  "build": {
    "appId": "com.jeevaanim.calculator",
    "files": [
      "electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  }
}